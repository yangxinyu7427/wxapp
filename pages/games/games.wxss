page {
  height: 100%;
  width: 100%;
}
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  background: #fff;
  color: #000;
}

.nav-title {
  position: absolute;
  text-align: center;
  max-width: 327rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 35rpx;
  color: #2c2b2b;
  font-weight: 400;
}

.nav-capsule {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  width: 50rpx;
  justify-content: space-around;
  /* border-radius: 50%; */
  margin-top: 54rpx;
  z-index: 999999999;
}

.swiper {
    height: 500rpx;
    width: 90%;
    margin: 0 auto;
    border-radius: 20rpx;
    /* background-color: red; */
    overflow: hidden;
    /* position: fixed; */
}
swiper {
    height: 500rpx;
}
.slide-image {
  height: 100%;
  width: 100%;
  display: block;
}
.van-cell {
  height: 200rpx !important;
  border-radius: 20rpx !important;
  width: 90% !important;
  background-color: aliceblue !important;
  margin: 0 auto;
  margin-top: 20rpx;
}
